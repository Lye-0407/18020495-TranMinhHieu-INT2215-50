#include <iostream>

using namespace std;

int n;

bool isPrime[1000005];
//isPrime[i] = True => i la so nguyen to
//isPrime[i] = False => i khong phai so nguyen to

void initPrime()
{
    isPrime[0] = False;
    isPrime[1] = False;
    int i = 2;

    while (i <= 1000000)
        if(isPrime[i])
        {
        int j = i * i;
            while j <= 1000000)
            {
                isPrime[j] = False;
                j += i;
            }
            i++;
    }
}